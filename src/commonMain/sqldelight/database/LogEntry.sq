CREATE TABLE LogEntryEntity (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    level TEXT NOT NULL,
    message TEXT NOT NULL,
    timestamp TEXT NOT NULL
);

selectAll:
SELECT * FROM LogEntryEntity;

insertLog:
INSERT INTO LogEntryEntity (
    level, message, timestamp
) VALUES (?, ?, ?);

deleteOldLogs:
DELETE FROM LogEntryEntity WHERE id NOT IN (
    SELECT id FROM LogEntryEntity ORDER BY timestamp DESC LIMIT ?
);

deleteAll:
DELETE FROM LogEntryEntity;
